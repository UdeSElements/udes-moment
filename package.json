  "description": "UdeS moment element wrapper for Moment.js",
  "author": "Universit√© de Sherbrooke",
  "license": "MIT",
  "keywords": [
    "moment",
    "polymer",
    "udes",
    "webcomponents"
  ],
  "devDependencies": {
    "eslint-config-udes": "^0.2.1",
    "npm-run-all": "^4.1.2",
    "pre-commit": "^1.2.2"
